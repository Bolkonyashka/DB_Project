@{
    var locCode = Request.QueryString["street"];

    var sql = @"SELECT count(NAME) FROM [BD_Project_DB].[dbo].houses "
        + " WHERE CODE LIKE @0 + '%'";

    using (var db = Database.Open("master"))
    {
        var data = db.QueryValue(sql, locCode);
        Json.Write(data, Response.Output);
    }

}
